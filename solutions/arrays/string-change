let fs = require("fs");
let data = fs.readFileSync(0, 'utf-8');
let idx = 0;
data = data.split('\n');

function readLine() {
  idx++;
  return data[idx - 1];
}

// -------- Do NOT edit anything above this line ----------
// Use readLine() for taking input, it will read one line of from the input  and is stored in string format
let stringOne = readLine().split("");
let stringTwo = readLine().split("");
stringOne.sort();
stringTwo.sort();
let differentCharacters = 1;
for(let index=0;index<stringOne.length;index++)
{
    if(stringOne[index] != stringTwo[index])
    {
        differentCharacters=0;
        break;
    }
}
if(stringOne.length != stringTwo.length)
{
    console.log(0);
}
else{
    console.log(differentCharacters);
}
