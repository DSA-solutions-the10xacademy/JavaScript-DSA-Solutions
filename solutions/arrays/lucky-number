let fs = require("fs");
let data = fs.readFileSync(0, 'utf-8');
let idx = 0;
data = data.split('\n');

function readLine() {
	idx++;
	return data[idx - 1];
}

// -------- Do NOT edit anything above this line ----------

function findLuckyNumber(nums) {
	// implement this function

	let lastElement = -1, numberOfOccurences = 0;
	for (let index = 0; index < nums.length; index++) {
		if (nums[index] === lastElement) {
			numberOfOccurences++;
		}
		else {
			if (numberOfOccurences === lastElement) {
				return numberOfOccurences;
			}
			lastElement = nums[index];
			numberOfOccurences = 1;
		}

	}
	if (numberOfOccurences === lastElement) {
		return numberOfOccurences;
	}
	return -1;
}
// DO NOT change anything below this line
let numElems = parseInt(readLine());
let inputArr = [];
for (let index = 0; index < numElems; index++) {
	inputArr.push(parseInt(readLine()));
}
console.log(findLuckyNumber(inputArr));
